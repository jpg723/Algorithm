   "source": [
    "N, M, K = map(int, input().split())\n",
    "\n",
    "# 좌석 상태 입력\n",
    "sit = [input() for _ in range(N)]\n",
    "\n",
    "result = 0\n",
    "\n",
    "for i in range(N):\n",
    "    for j in range(M - K + 1):\n",
    "        \n",
    "        # 1이 없는 경우에 result 증가\n",
    "        if '1' not in sit[i][j:j+K]:\n",
    "            result += 1\n",
    "\n",
    "print(result)"
   ]
