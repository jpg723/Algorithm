   "source": [
    "# https://www.acmicpc.net/problem/2839\n",
    "\n",
    "N = int(input())\n",
    "\n",
    "count = 0;\n",
    "\n",
    "while True:\n",
    "\n",
    "    if(N <= 0):\n",
    "        break\n",
    "\n",
    "    if(N % 5 > N % 3):\n",
    "        N -= 3\n",
    "        count += 1\n",
    "\n",
    "\n",
    "    else:\n",
    "        N -= 5\n",
    "        count += 1\n",
    "\n",
    "if(N < 0) :\n",
    "    count = -1;\n",
    "\n",
    "print(count);"
   ]
  }
